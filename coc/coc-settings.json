{
  "languageserver": {
    "golang": {
      "command": "gopls",
      "rootPatterns": ["go.mod", ".vim/", ".git/", ".svn/", ".hg/"],
      "filetypes": ["go"]
    }
  },
  "snippets.ultisnips.directories": [
    "UltiSnips",
    "gosnippets/UltiSnips"
  ],
